{
  "mcpServers": {
    "pdf-extraction": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@xraywu/pdf-extraction-mcp"],
      "env": {}
    },
    "document-forge": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@cablate/mcp-doc-forge"],
      "env": {}
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "./"],
      "env": {}
    },
    "office-powerpoint": {
      "type": "stdio",
      "command": "uvx",
      "args": ["--from", "office-powerpoint-mcp-server", "ppt_mcp_server"],
      "env": {}
    }
  }
}
