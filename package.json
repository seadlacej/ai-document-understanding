{
  "name": "ai-document-understanding",
  "version": "1.0.0",
  "description": "Complete document content extraction with deep AI analysis of images and videos",
  "type": "module",
  "scripts": {
    "setup": "mkdir -p uploads output/text output/images output/videos output/analysis output/final temp logs",
    "clean": "rm -rf temp/*",
    "test": "echo 'Upload files to ./uploads folder for analysis'",
    "check:libreoffice": "node src/utils/check-libreoffice.js",
    "docker:build": "docker build -t ai-document-understanding .",
    "docker:build:prod": "docker build -f Dockerfile.prod -t ai-document-understanding:prod .",
    "docker:up": "docker-compose up -d",
    "docker:up:build": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:app": "docker-compose logs -f app",
    "docker:clean": "docker-compose down -v",
    "docker:prod": "docker-compose --profile production up -d",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "adm-zip": "^0.5.10",
    "archiver": "^7.0.1",
    "axios": "^1.6.7",
    "bits-ui": "^2.8.11",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "mammoth": "^1.6.0",
    "node-unzip-2": "^0.2.8",
    "openai": "^4.28.0",
    "pdf-parse": "^1.1.1",
    "pocketbase": "^0.26.1",
    "sharp": "^0.34.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.2",
    "@modelcontextprotocol/sdk": "latest",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.0.0",
    "@types/sharp": "^0.32.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.525.0",
    "postcss": "^8.5.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  },
  "keywords": [
    "document-processing",
    "pdf-extraction",
    "docx-extraction",
    "pptx-extraction",
    "image-analysis",
    "video-analysis",
    "ai-understanding",
    "mcp"
  ],
  "author": "",
  "license": "MIT"
}
